{
  "name": "railserd",
  "private": true,
  "scripts": {
    "build": "esbuild app/javascript/application.js --bundle --outdir=app/assets/builds --public-path=/assets --sourcemap --define:process.env.NODE_ENV=\"production\" --minify",
    "build:dev": "esbuild app/javascript/application.js --bundle --outdir=app/assets/builds --public-path=/assets --sourcemap",
    "build:watch": "esbuild app/javascript/application.js --bundle --outdir=app/assets/builds --public-path=/assets --sourcemap --watch"
  },
  "devDependencies": {
    "esbuild": "^0.21.0"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.4",
    "d3": "^7.9.0",
    "lucide": "^0.468.0"
  }
}


